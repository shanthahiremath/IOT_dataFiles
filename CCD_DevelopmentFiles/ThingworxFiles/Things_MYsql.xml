]]></sql></Row></Rows></ConfigurationTable></ConfigurationTables></ServiceImplementation><ServiceImplementation description="" handlerName="SQLQuery" name="getDailyCountbyIDMen3"><ConfigurationTables><ConfigurationTable description="SQLQuery" isMultiRow="false" name="Query" ordinal="0"><DataShape><FieldDefinitions><FieldDefinition baseType="NUMBER" description="maxItems" name="maxItems" ordinal="0"/><FieldDefinition baseType="NUMBER" description="timeout" name="timeout" ordinal="0"/><FieldDefinition baseType="STRING" description="sql" name="sql" ordinal="0"/></FieldDefinitions></DataShape><Rows><Row><maxItems>500.0</maxItems><timeout>60.0</timeout><sql><![CDATA[select max(daily_count) as Tea from trans_details where trans_details.machine_id='MAC0001' and menu_id='Men003']]></sql></Row></Rows></ConfigurationTable></ConfigurationTables></ServiceImplementation><ServiceImplementation description="" handlerName="SQLQuery" name="getDailyCountbyIDMen4"><ConfigurationTables><ConfigurationTable description="SQLQuery" isMultiRow="false" name="Query" ordinal="0"><DataShape><FieldDefinitions><FieldDefinition baseType="NUMBER" description="maxItems" name="maxItems" ordinal="0"/><FieldDefinition baseType="NUMBER" description="timeout" name="timeout" ordinal="0"/><FieldDefinition baseType="STRING" description="sql" name="sql" ordinal="0"/></FieldDefinitions></DataShape><Rows><Row><maxItems>100000.0</maxItems><timeout>100.0</timeout><sql><![CDATA[select max(daily_count) as Milk from trans_details where trans_details.machine_id='MAC0001' and menu_id='Men004']]></sql></Row></Rows></ConfigurationTable></ConfigurationTables></ServiceImplementation><ServiceImplementation description="" handlerName="SQLQuery" name="getAlertdetails"><ConfigurationTables><ConfigurationTable description="SQLQuery" isMultiRow="false" name="Query" ordinal="0"><DataShape><FieldDefinitions><FieldDefinition baseType="NUMBER" description="maxItems" name="maxItems" ordinal="0"/><FieldDefinition baseType="NUMBER" description="timeout" name="timeout" ordinal="0"/><FieldDefinition baseType="STRING" description="sql" name="sql" ordinal="0"/></FieldDefinitions></DataShape><Rows><Row><maxItems>500.0</maxItems><timeout>60.0</timeout><sql><![CDATA[select temp.alert_details as Alert_Details,temp.location as Location,sev.error_severity_name as Severity,temp.status as Status,
temp.start_time as Start_Time,temp.end_time as End_Time,temp.resolution_time as Resolution_Time,temp.actual_time as Actual_time from 
(select error.error_severity_id as error_severity_id,error.error_name as alert_details,loc.location_name as location,fail.status as status,
CONCAT(TIMESTAMPDIFF(hour,TIMESTAMP(STR_TO_DATE(fail.start_time, "%m-%d-%Y %H:%i")),TIMESTAMP(STR_TO_DATE(fail.end_time, "%m-%d-%Y %H:%i"))),':',
MOD(TIMESTAMPDIFF(MINUTE,TIMESTAMP(STR_TO_DATE(fail.start_time, "%m-%d-%Y %H:%i")),TIMESTAMP(STR_TO_DATE(fail.end_time, "%m-%d-%Y %H:%i"))),60)) 
as actual_time,fail.start_time as start_time,fail.end_time as end_time,error.resolution_time as resolution_time 
from failure_details as fail,error_list_details as error,location_details as loc 
where fail.error_id=error.error_id and fail.location_id=loc.location_id) as temp,error_severity_details as sev 
where temp.error_severity_id= sev.error_severity_id]]></sql></Row></Rows></ConfigurationTable></ConfigurationTables></ServiceImplementation><ServiceImplementation description="" handlerName="SQLQuery" name="getCapexandOpex"><ConfigurationTables><ConfigurationTable description="SQLQuery" isMultiRow="false" name="Query" ordinal="0"><DataShape><FieldDefinitions><FieldDefinition baseType="NUMBER" description="maxItems" name="maxItems" ordinal="0"/><FieldDefinition baseType="NUMBER" description="timeout" name="timeout" ordinal="0"/><FieldDefinition baseType="STRING" description="sql" name="sql" ordinal="0"/></FieldDefinitions></DataShape><Rows><Row><maxItems>500.0</maxItems><timeout>60.0</timeout><sql><![CDATA[select "capex" as amount_type,(count(distinct(machine_id))*(select menu_price from menu_details where menu_id="MEN008")) as total 
from trans_details where YEAR(STR_TO_DATE(created_time, "%m-%d-%Y"))="2015"
union
select "opex" as amount_type,sum(temp.menu_amt+exp_amt) as total from 
(select trans.machine_id, sum(trans.daily_count) as menu_total_count,(sum(trans.daily_count)*menu.menu_price) as menu_amt,
((select menu_price from menu_details where menu_id="MEN005")+(select menu_price from menu_details where menu_id="MEN006")+
(select menu_price from menu_details where menu_id="MEN007")) as exp_amt from trans_details as trans,menu_details as menu 
where YEAR(STR_TO_DATE(trans.created_time, "%m-%d-%Y"))="2015" and trans.menu_id=menu.menu_id group by trans.machine_id) as temp]]></sql></Row></Rows></ConfigurationTable></ConfigurationTables></ServiceImplementation><ServiceImplementation description="" handlerName="SQLQuery" name="getYearlyturnover"><ConfigurationTables><ConfigurationTable description="SQLQuery" isMultiRow="false" name="Query" ordinal="0"><DataShape><FieldDefinitions><FieldDefinition baseType="NUMBER" description="maxItems" name="maxItems" ordinal="0"/><FieldDefinition baseType="NUMBER" description="timeout" name="timeout" ordinal="0"/><FieldDefinition baseType="STRING" description="sql" name="sql" ordinal="0"/></FieldDefinitions></DataShape><Rows><Row><maxItems>500.0</maxItems><timeout>60.0</timeout><sql><![CDATA[select sum(temp1.2012) as "2012",sum(temp1.2013) as "2013",sum(temp1.2014) as "2014",sum(temp1.2015) as "2015" 
from
(select temp.menu_id,case when temp.trans_year="2012" then temp.mul_value end as "2012",case when temp.trans_year="2013" then temp.mul_value end as "2013",
case when temp.trans_year="2014" then temp.mul_value end as "2014",case when temp.trans_year="2015" then temp.mul_value end as "2015" 
from(SELECT trans.menu_id,YEAR(STR_TO_DATE(trans.created_time, "%m-%d-%Y")) as trans_year,sum(trans.daily_count) as daily_count,menu.menu_price,
(sum(trans.daily_count)*menu.menu_price) as mul_value from trans_details as trans,menu_details as menu 
where trans.menu_id=menu.menu_id group by trans_year,menu_id) as temp) 
as temp1;]]></sql></Row></Rows></ConfigurationTable></ConfigurationTables></ServiceImplementation><ServiceImplementation description="" handlerName="SQLQuery" name="geterrotrends"><ConfigurationTables><ConfigurationTable description="SQLQuery" isMultiRow="false" name="Query" ordinal="0"><DataShape><FieldDefinitions><FieldDefinition baseType="NUMBER" description="maxItems" name="maxItems" ordinal="0"/><FieldDefinition baseType="NUMBER" description="timeout" name="timeout" ordinal="0"/><FieldDefinition baseType="STRING" description="sql" name="sql" ordinal="0"/></FieldDefinitions></DataShape><Rows><Row><maxItems>500.0</maxItems><timeout>60.0</timeout><sql><![CDATA[select temp.error_name,((temp.error_count/temp.error_total)*100) as error_percentage from 
(select error.error_name,count(fail.error_id) as error_count,(select count(error_id) from failure_details where YEAR(STR_TO_DATE(start_time, "%m-%d-%Y"))="2015") as error_total 
from failure_details as fail,error_list_details as error 
where YEAR(STR_TO_DATE(start_time, "%m-%d-%Y"))="2015" and fail.error_id=error.error_id group by fail.error_id) as temp]]></sql></Row></Rows></ConfigurationTable></ConfigurationTables></ServiceImplementation><ServiceImplementation description="" handlerName="SQLQuery" name="getMachineSettingsbyID"><ConfigurationTables><ConfigurationTable description="SQLQuery" isMultiRow="false" name="Query" ordinal="0"><DataShape><FieldDefinitions><FieldDefinition baseType="NUMBER" description="maxItems" name="maxItems" ordinal="0"/><FieldDefinition baseType="NUMBER" description="timeout" name="timeout" ordinal="0"/><FieldDefinition baseType="STRING" description="sql" name="sql" ordinal="0"/></FieldDefinitions></DataShape><Rows><Row><maxItems>500.0</maxItems><timeout>60.0</timeout><sql><![CDATA[select heating_time,initialization_time,ideally_esp,pump_lat,dispense_time,milk_cap,grind_cap,grind_lat,flow_meter_parameter,tea_water,ideally_tea,milk_pos_lat,
hot_water,milk,milk_pre_cap,esp_flm_pls,lat_flm_pls,ideally_cap,ideally_lat
from setting_details,machine_details where 
setting_details.machine_id='MAC0001' and setting_details.machine_id=machine_details.machine_id]]></sql></Row></Rows></ConfigurationTable></ConfigurationTables></ServiceImplementation><ServiceImplementation description="" handlerName="SQLQuery" name="getQtrlyturnover"><ConfigurationTables><ConfigurationTable description="SQLQuery" isMultiRow="false" name="Query" ordinal="0"><DataShape><FieldDefinitions><FieldDefinition baseType="NUMBER" description="maxItems" name="maxItems" ordinal="0"/><FieldDefinition baseType="NUMBER" description="timeout" name="timeout" ordinal="0"/><FieldDefinition baseType="STRING" description="sql" name="sql" ordinal="0"/></FieldDefinitions></DataShape><Rows><Row><maxItems>500.0</maxItems><timeout>60.0</timeout><sql><![CDATA[select sum(temp1.Q1) as "Q1",sum(temp1.Q2) as "Q2",sum(temp1.Q3) as "Q3",sum(temp1.Q4) as "Q4" from (select temp.menu_id,case when (temp.trans_month="1" || temp.trans_month="2" || temp.trans_month="3") then temp.mul_value end as "Q1",case when (temp.trans_month="4" || temp.trans_month="5" || temp.trans_month="6") then temp.mul_value end as "Q2",case when (temp.trans_month="7" || temp.trans_month="8" || temp.trans_month="9") then temp.mul_value end as "Q3",case when (temp.trans_month="10" || temp.trans_month="11" || temp.trans_month="12") then temp.mul_value end as "Q4" from(SELECT trans.menu_id,month(STR_TO_DATE(trans.created_time, "%m-%d-%Y")) as trans_month,sum(trans.daily_count) as daily_count,menu.menu_price,(sum(trans.daily_count)*menu.menu_price) as mul_value from trans_details as trans,menu_details as menu where trans.menu_id=menu.menu_id and YEAR(STR_TO_DATE(trans.created_time, "%m-%d-%Y"))="2015" group by trans_month,menu_id) as temp)as temp1;]]></sql></Row></Rows></ConfigurationTable></ConfigurationTables></ServiceImplementation><ServiceImplementation description="" handlerName="SQLQuery" name="getExpenditure"><ConfigurationTables><ConfigurationTable description="SQLQuery" isMultiRow="false" name="Query" ordinal="0"><DataShape><FieldDefinitions><FieldDefinition baseType="NUMBER" description="maxItems" name="maxItems" ordinal="0"/><FieldDefinition baseType="NUMBER" description="timeout" name="timeout" ordinal="0"/><FieldDefinition baseType="STRING" description="sql" name="sql" ordinal="0"/></FieldDefinitions></DataShape><Rows><Row><maxItems>500.0</maxItems><timeout>60.0</timeout><sql><![CDATA[select ttt.menu_name,((ttt.menu_total_amt/ttt.tot)*100) as exp_percentage from 
(select tt.menu_name,tt.menu_total_amt,(select sum(ttt.menu_total_amt) from 
((select "total" as menu_name,sum(temp.menu_amt) as menu_total_amt from 
(select menu.menu_id,menu.menu_name, sum(trans.daily_count) as menu_total_count,(sum(trans.daily_count)*menu.menu_price) as menu_amt from trans_details as trans,menu_details as menu 
where YEAR(STR_TO_DATE(trans.created_time, "%m-%d-%Y"))="2015" and trans.menu_id=menu.menu_id group by trans.menu_id) as temp) 
union 
(select menu_name,menu_price from menu_details where menu_id IN("MEN005","MEN006","MEN007"))) as ttt) as tot from 
((select "Storgae/Materials" as menu_name,sum(temp.menu_amt) as menu_total_amt from 
(select menu.menu_id,menu.menu_name, sum(trans.daily_count) as menu_total_count,(sum(trans.daily_count)*menu.menu_price) as menu_amt from trans_details as trans,menu_details as menu 
where YEAR(STR_TO_DATE(trans.created_time, "%m-%d-%Y"))="2015" and trans.menu_id=menu.menu_id group by trans.menu_id) as temp) 
union 
(select menu_name,menu_price from menu_details where menu_id IN("MEN005","MEN006","MEN007"))) as tt) as ttt

]]></sql></Row></Rows></ConfigurationTable></ConfigurationTables></ServiceImplementation><ServiceImplementation description="" handlerName="SQLQuery" name="getDailyCountbyMen2"><ConfigurationTables><ConfigurationTable description="SQLQuery" isMultiRow="false" name="Query" ordinal="0"><DataShape><FieldDefinitions><FieldDefinition baseType="NUMBER" description="maxItems" name="maxItems" ordinal="0"/><FieldDefinition baseType="NUMBER" description="timeout" name="timeout" ordinal="0"/><FieldDefinition baseType="STRING" description="sql" name="sql" ordinal="0"/></FieldDefinitions></DataShape><Rows><Row><maxItems>500.0</maxItems><timeout>60.0</timeout><sql><![CDATA[select max(daily_count) as Cappuccino from trans_details where trans_details.machine_id='MAC0001' and menu_id='Men002']]></sql></Row></Rows></ConfigurationTable></ConfigurationTables></ServiceImplementation></ServiceImplementations><Subscriptions/></ThingShape><PropertyBindings/><RemotePropertyBindings/><RemoteServiceBindings/><RemoteEventBindings/><AlertConfigurations><AlertDefinitions name="SalesValue"/></AlertConfigurations><ImplementedShapes/><ThingProperties/><ConfigurationChanges><ConfigurationChange changeAction="CREATE" changeReason="header : name : updated, thingTemplate : updated, description : updated&#10;documentation : updated&#10;configuration : ConnectionInfo : updated&#10;" timestamp="2015-11-02T11:39:26.771+05:30" user="Administrator"/><ConfigurationChange changeAction="MODIFY" changeReason="services :  : ServiceType updated, Output Updated, getcity : Code Updated, Service Added, Output Updated&#10;" timestamp="2015-11-02T11:45:34.747+05:30" user="Administrator"/><ConfigurationChange changeAction="MODIFY" changeReason="configuration : ConnectionInfo : updated&#10;" timestamp="2015-11-02T11:46:30.495+05:30" user="Administrator"/><ConfigurationChange changeAction="MODIFY" changeReason="services : getcity : Output Updated, Code Updated&#10;" timestamp="2015-11-02T11:47:00.320+05:30" user="Administ